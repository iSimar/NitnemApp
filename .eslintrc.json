{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/forbid-prop-types": 0,
        "no-use-before-define": 0,
        "comma-dangle": ["error", "never"],
        "class-methods-use-this": 0,
        "no-nested-ternary": 0,
        "no-plusplus": 0
    }
}